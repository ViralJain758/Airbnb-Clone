<% layout("/layouts/boilerplate.ejs") %>
<body>

<div class="cards">
<% for(let list of listings){ %>
    <a href="/listings/<%= list._id %>" class="card-link">
    <div class="card">
    <img src="<%= list.image.url %>" alt="Image of <%= list.title %>">
    <b><p class="list-title"><%= list.title %></p></b>
    <p class="list-price">â‚¹ <%= list.price.toLocaleString("en-IN") %> / night</p>
    </div>
    </a>
<% }  %> 
</div>

</body>