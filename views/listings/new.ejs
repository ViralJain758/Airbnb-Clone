<% layout("/layouts/boilerplate.ejs") %>
<body>
  <h3>Create A New Listing</h3>
  <form method="POST" action="/listings">
    <input name="listing[title]" , placeholder="Enter Title" />
    <br />
    <textarea
      name="listing[description]"
      ,
      placeholder="Enter Description"
    ></textarea>
    <br />
    <input name="listing[image]" , placeholder="Enter Image URL" />
    <br />
    <input name="listing[price]" , placeholder="Enter Price" type="number" />
    <br />
    <input name="listing[location]" , placeholder="Enter Location" />
    <br />
    <input name="listing[country]" , placeholder="Enter Country" />
    <br />
    <button>Add</button>
  </form>
</body>
